{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "action_id": {
      "type": "integer"
    },
    "action": {
      "type": "string"
    },
    "selector": {
      "type": "object",
      "properties": {
        "cluster": {
          "type": "string"
        }
      },
      "required": [
        "cluster"
      ]
    },
    "status": {
      "type": "string"
    },
    "jobs": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "url": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "action_id",
    "action",
    "selector",
    "status",
    "jobs",
    "url"
  ]
}
