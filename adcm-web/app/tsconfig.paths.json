{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@pages/*": [
        "./components/pages/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@constants": [
        "./constants"
      ],
      "@uikit": [
        "./components/uikit/index"
      ],
